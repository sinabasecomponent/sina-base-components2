{
  "compilerOptions": {
    "target": "ES5",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "declaration": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "emitDeclarationOnly": true,
    "jsx": "react-jsxdev",
    "resolveJsonModule": true,
    "esModuleInterop": true
  }
}
